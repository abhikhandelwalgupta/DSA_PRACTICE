#include<iostream>
using namespace std;


int main(){

   
    int n,i=1,j=1,val=0;
    cin>>n;
    
    while(i<=n) {
        val=i ,j=1;
        while(j<=i) {
            
            cout<<val;
            j++,val++;
        }
        cout<<endl;
        i++;
    }
    
    
  
}


