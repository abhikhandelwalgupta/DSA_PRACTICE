#include<iostream>
using namespace std;


int main(){

    int n=0,i=1,j=1,val=0;
    cin>>n;
    
    while(i<=n) {
        j=1;
        val=i;
        while(j<=i) {
            cout<<val;
            val--,j++;            
        }
        i++;
        cout<<endl;
    } 
}


