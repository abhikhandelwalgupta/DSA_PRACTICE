E
DE
CDE
BCDE
ABCDE
---------------------


#include<iostream>
using namespace std;


int main() {
    
    int n,j=1,i=1;
    char c;
    cin>>n;
    
    while(i<=n) {
        j=1;
        c='A'+n-i;
        while(j<=i) {
            cout<<c;
            c++;
            j++;
        }
        i++;
        cout<<endl;
    }
    
}

    

