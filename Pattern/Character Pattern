A
BC
CDE
DEFG

-----------------------------

#include<iostream>
using namespace std;


int main(){

  int n=0,i=1,j=1;
    cin>>n;
  char c;
    
    while(i<=n) {
        c= 'A' + i-1;
        j=1;
        while(j<=i) {
            cout<<c;
            c++;
            j++;
        }
        i++;
        cout<<endl;
    }
}


