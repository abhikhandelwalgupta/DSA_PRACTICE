*
**
***
****



-------------------------------------------------------------

#include<iostream>
using namespace std;


int main(){

    int n,j=1,i=1;
    cin>>n;
    
    while(i<=n) {
        j=1;
        while(j<=i){
            cout<<"*";
            j++;
        }
        i++;
        cout<<endl;
    }
  
}


