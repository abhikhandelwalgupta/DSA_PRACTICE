 A
 BB
 CCC
 
 --------------------------------
 
 #include<iostream>
using namespace std;


int main(){
    

    int n,j=1,i=1;
    char c='A';
    cin>>n;
    
    while(i<=n) {
        j=1;
        while(j<=i) {
            cout<<c;
            j++;
        }
        c++;
        i++;
        cout<<endl;
        
    }
    
}

